{
  "name": "nodepop",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "DEBUG=nodepop:* nodemon ./bin/www",
    "hints": "jshint . && ./node_modules/.bin/jscs . || true",
    "installDB": "node install_db.js",
    "e2e": "cross-env LOG_FORMAT=nolog mocha **/*.e2e.js --recursive --watch --timeout 2000"
  },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "cross-env": "^5.1.1",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "hash.js": "^1.0.3",
    "i18n": "^0.8.3",
    "mocha": "^4.0.1",
    "mockgoose": "^7.3.3",
    "mongoose": "^4.13.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0",
    "validator": "^4.0.6"
  },
  "devDependencies": {
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "supertest": "^3.0.0"
  }
}
